if(VehicleSpeed > 0)
{
    if(VehicleBrake == 1)
    {
        VehicleSpeed -= 1;
    }
    else if(VehicleBrake == 2)
    {
        VehicleSpeed -= 2;
    }
    else if(VehicleBrake == 3)
    {
        VehicleSpeed -= 3;
    }
    else if(VehicleBrake == 4)
    {
        VehicleSpeed -= 4;
    }
}
else
{
    if(VehicleBrake != 0)
    {
        VehicleSpeed = 0;
        VehicleS.Content = VehicleSpeed;
    }
}